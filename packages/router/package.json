    "rollup": "rollup -c",
    "postrollup": "tsc --declaration --emitDeclarationOnly"
  },
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@aurelia/kernel": "dev",
    "@aurelia/metadata": "dev",
    "@aurelia/platform": "dev",
    "@aurelia/platform-browser": "dev",
    "@aurelia/route-recognizer": "dev",
    "@aurelia/runtime": "dev",
    "@aurelia/runtime-html": "dev"
  },
  "devDependencies": {
    "typescript": "^4.0.3"
  },
  "engines": {
    "node": ">=14.15.0"
  }
